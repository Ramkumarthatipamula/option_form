  <body>
      <div class="container">
        <div class="row">
          <center><img src="<%= asset_path 'pjtsau_logo.png' %>" width="100" height="100" class="d-inline-block align-top" alt="">
          <font class="" color="green" size="5">Professor Jayashankar Telangana State Aggricultural University(PJTSAU)</font></center>
          <% if user_signed_in? %>
            <a class="btn btn-warning pull-right" href="/users/sign_out" data-method="delete">Sign out</a>
            <button type="button" class="btn btn-primary pull-right"><%= current_user.present? ? current_user.email : '' %></button>
