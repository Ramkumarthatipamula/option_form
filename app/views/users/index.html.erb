<div class="container">
  <div class="pull-right" id="new_user">
    <%= link_to 'New Application', new_user_path, :class => "btn btn-large btn-primary" %>
  </div>
  <table class="table table-striped table-bordered" width="100%" cellspacing="0" id="users-list" style="border-radius: 4px;">
    <thead>
      <tr style="text-align:center;">
        <th>Name</th>
        <th>Email</th>
        <th>Mobile Phone</th>
        <th>Aadhar Number</th>
        <th>Pdf File</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %> </td>
          <td><%= user.email %></td>
          <td><%= user.mobile_no %></td>
          <td><%= user.aadhar_no %></td>
          <td><%= user.pdf_file %></td>
        </tr>
        <% end %>
    </tbody>
  </table>
