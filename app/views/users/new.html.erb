<div class="container">
  <div class="bs-example">
      <form action="/merit2017/application_check" accept-charset="UTF-8" method="post" id="onboardingform" multipart="true">
        <h3 align="center" style="color: green;">Application Form</h3><br
        <div class="row">
          <div class="form-group row field">
            <label for="user_name" class="col-xs-2 col-form-label">Name</label>
            <div class="col-xs-10">
              <input type="text" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="user_email" class="col-xs-2 col-form-label">Email</label>
            <div class="col-sm-10">
              <input type="text" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="user_mobilephone" class="col-xs-2 col-form-label">Mobile Phone</label>
            <div class="col-sm-10">
              <input type="number" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="user_email" class="col-xs-2 col-form-label">Aadhar Number</label>
            <div class="col-sm-10">
              <input type="number" class="form-control">
            </div>
          </div>
          <div class="form-group row">
           <label for="image" class="col-xs-2 col-form-label">Uplode File</label>
           <div class="col-sm-10">
             <input type="file" accept="image/*;" name='pdf_file' multiple class="form-control-file" id="pdf_file">
             <small id="pdf_help" class="form-text text-muted">Please upload pdf file</small>
           </div>
          </div>
          <input type="submit" name="commit" value="Submit" class="btn btn-success pull-right" />
        </div>
      </form>
  </div>
</div><!-- container -->
